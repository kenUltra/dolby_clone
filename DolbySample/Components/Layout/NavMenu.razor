@code {
    [Parameter]
    public string? navStyle {get; set;} = "";
    [Parameter]
    public bool? isLading { get; set;} = false;

    private string menuContent {get; set;} = "";

    public string showHead() {
        string res = isLading == true ? "bolby-nav" : "fsc-bolby-nav";
        return res;
    }
    private void ToggleMenu(){
        if(menuContent == ""){
            menuContent = "opn-mn";
        } else {
            menuContent = "";
        }
    }

}

